<!-- hour_header must be sorted based on hour
entry can only be only one of hour or day/month type
day_header entries have a higher priority then hour entries -->

<daylight_headers>
    <meta_data creator="rock_kellan" />
    <hour_header hour="6" image="outline_sunrise" />
    <hour_header hour="9" image="outline_morning" />
    <hour_header hour="11" image="outline_noon" />
    <hour_header hour="13" image="outline_afternoon" />
    <hour_header hour="19" image="outline_evening" />
    <hour_header hour="21" image="outline_night" />

    <!-- Special day headers -->
    <day_header day="25" month="12" image="outline_christmas" />
    <day_header day="31" month="12" image="outline_newyeareve" />
</daylight_headers>
